class Solution {
    public int countSeniors(String[] details) {
        int i;
        int j;
        String str="";
        int k=0;
        int l=details.length;
        for(i=0;i<l;i++)
        {
            str=details[i].substring(11,13);
            int age=Integer.valueOf(str);
            if(age>60)
            k++;
        }
        if(k==0)
        return 0;
        else
        return k;
    }
}
