class Solution 
{
    public boolean isIsomorphic(String s, String t) 
    {
      for(int i =0;i<s.length();i++)
        {
            char b=s.charAt(i);
            char b1=t.charAt(i);
            if(s.lastIndexOf(b)!=t.lastIndexOf(b1))
            return false;
        }   
        return true;
    }
}
